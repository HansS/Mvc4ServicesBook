<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
               namespace="MVC4ServicesBook.Data.Model"
               assembly="MVC4ServicesBook.Data"
               schema="dbo" >
    
    <class name="Priority" table="Priority">

        <id name="PriorityId" type="Int64" unsaved-value="0">
          <generator class="identity"/>
        </id>

        <version name="Timestamp" generated="always" unsaved-value="null" type="BinaryBlob" >
          <column name="ts" not-null="true" sql-type="rowversion"/>
        </version>
  
        <property name="Name" not-null="true" type="String" />
        <property name="Ordinal" not-null="true" type="Int32" />

    </class>
    
</hibernate-mapping>